{
  "name": "kolm-start",
  "description": "TanStack Start starter with tRPC, Drizzle ORM and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vinxi build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx app/db/seed.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vinxi dev",
    "format": "prettier --write .",
    "lint": "eslint app",
    "postinstall": "drizzle-kit push",
    "start": "NODE_ENV=production vinxi start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.0-beta.8",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@tanstack/react-router": "^1.91.3",
    "@tanstack/react-router-with-query": "^1.92.1",
    "@tanstack/router-devtools": "^1.92.1",
    "@tanstack/start": "^1.91.3",
    "@trpc/client": "11.0.0-rc.666",
    "@trpc/react-query": "11.0.0-rc.666",
    "@trpc/server": "11.0.0-rc.666",
    "better-auth": "^1.1.8",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.0.0-beta.8",
    "vinxi": "^0.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
