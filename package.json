{
  "name": "kolm-start",
  "description": "TanStack Start starter with tRPC, Drizzle ORM, better-auth and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.4.9",
  "type": "module",
  "keywords": [
    "better-auth",
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vinxi build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx app/db/seed.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vinxi dev",
    "format": "prettier --write .",
    "lint": "eslint app",
    "postinstall": "drizzle-kit push",
    "start": "node --env-file=.env ./node_modules/.bin/vinxi start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/postcss": "4.1.3",
    "@tanstack/react-query": "5.74.0",
    "@tanstack/react-query-devtools": "5.74.0",
    "@tanstack/react-router": "1.115.3",
    "@tanstack/react-router-devtools": "1.115.3",
    "@tanstack/react-router-with-query": "1.115.3",
    "@tanstack/react-start": "1.115.3",
    "@trpc/client": "11.1.0",
    "@trpc/server": "11.1.0",
    "@trpc/tanstack-react-query": "11.1.0",
    "better-auth": "1.2.7",
    "dotenv": "16.5.0",
    "drizzle-orm": "0.41.0",
    "pg": "8.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "superjson": "2.2.2",
    "tailwindcss": "4.1.3",
    "vinxi": "0.5.4",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@types/pg": "8.11.12",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.3.4",
    "drizzle-kit": "0.30.6",
    "eslint": "9.24.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite-tsconfig-paths": "5.1.4"
  }
}
