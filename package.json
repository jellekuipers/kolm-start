{
  "name": "kolm-start",
  "description": "TanStack Start starter with tRPC, Drizzle ORM, better-auth and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.4.11",
  "type": "module",
  "keywords": [
    "better-auth",
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vinxi build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx app/db/seed.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vinxi dev",
    "format": "prettier --write .",
    "lint": "eslint app",
    "postinstall": "drizzle-kit push",
    "start": "node --env-file=.env ./node_modules/.bin/vinxi start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-query": "^5.74.11",
    "@tanstack/react-query-devtools": "^5.74.11",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "@tanstack/react-router-with-query": "^1.119.0",
    "@tanstack/react-start": "^1.119.0",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@trpc/tanstack-react-query": "^11.1.2",
    "better-auth": "^1.2.7",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "pg": "^8.15.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.5",
    "vinxi": "^0.5.6",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/pg": "^8.11.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
