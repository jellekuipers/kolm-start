{
  "name": "kolm-start",
  "description": "TanStack Start starter with tRPC, Drizzle ORM, better-auth and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.6.1",
  "type": "module",
  "keywords": [
    "better-auth",
    "biome",
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx src/db/seed.ts ",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "postinstall": "drizzle-kit push",
    "start": "node --env-file=.env .output/server/index.mjs",
    "typecheck": "npx tsc --noEmit "
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/react-router-devtools": "^1.131.27",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.131.27",
    "@trpc/client": "^11.5.0",
    "@trpc/server": "^11.5.0",
    "@trpc/tanstack-react-query": "^11.5.0",
    "better-auth": "^1.3.7",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "superjson": "^2.2.2",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
